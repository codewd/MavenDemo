<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.mewin.entity">
	<class name="Tuzai">
		<id name="id">
			<generator class="native" />
		</id>
		<property name="erbiao" length="15" />
		<property name="riqi" type="date" />
		<property name="jgrq" type="date" />
		<property name="buwei" length="15" />
		<property name="weight" length="15" />
	</class>
</hibernate-mapping>
